  "license": "MIT",
  "lint-staged": {
    "(apps|packages)/**/*.{js,ts,jsx,tsx}": [
      "prettier --write",
      "eslint --fix --max-warnings=1000"
    ],
    "*.json": [
      "prettier --write"
    ],
    "!packages/labels-forms/**/*": []
  },
  "private": true,
  "scripts": {
    "build": "turbo run build --filter=./packages/camel",
    "build:packages": "turbo run build --filter=./packages/*",
    "clean": "turbo run clean && rimraf ./node_modules && rimraf ./package-lock.json",
    "db:build": "run-s db:kill db:start setup",
    "db:function:new": "npm run db:function:new -w @zeak/database",
    "db:kill": "npm run db:kill -w @zeak/database",
    "db:kill:redis": "npm run db:kill -w @zeak/redis",
    "db:migrate": "npm run db:migrate -w @zeak/database",
    "db:migrate:force": "turbo run db:migrate:force --no-cache",
    "db:migrate:new": "npm run db:migrate:new -w @zeak/database",
    "db:pull": "turbo run db:pull",
    "db:push": "turbo run db:push",
    "db:restart": "run-s db:stop db:start",
    "db:seed": "turbo run db:seed --no-cache",
    "db:seed-triggers": "tsx packages/database/src/seed-triggers.ts",
    "db:start": "npm run db:start -w @zeak/database",
    "db:start:redis": "npm run db:start -w @zeak/redis",
    "db:stop": "npm run db:stop -w @zeak/database",
    "db:types": "npm run db:types -w @zeak/database",
    "prisma:generate": "turbo run prisma:generate --filter=./apps/zeak",
    "deploy": "turbo run deploy",
    "dev": "turbo run dev --filter=./apps/zeak --filter=./packages/",
    "start": "turbo run start",
    "dev:packages": "turbo run dev --filter=./packages/*",
    "format": "prettier --write \"**/*.{ts,tsx,md}\"",
    "lint": "turbo run lint",
    "postdb:build": "tsx scripts/generate-db-types.ts",
    "prepare": "npx husky install",
    "setup": "turbo run setup",
    "test": "turbo run test",
    "test:preview": "turbo run test:preview",
    "serve:build": "remix-serve ./apps/zeak/build/server/nodejs-eyJydW50aW1lIjoibm9kZWpzIn0/index.js"
  },
  "workspaces": [
    "apps/*",
    "packages/*",
    "shared",
    "www"
  ],
